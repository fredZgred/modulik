{
  "name": "watch-module-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:client": "NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "babel -d dist src/App.jsx && cpx \"src/{server,ssr}.js\" dist",
    "start": "nodemon -q -w src src/server.js -- 3000",
    "start:prod": "NODE_ENV=production node dist/server.js 3000"
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0"
  }
}
